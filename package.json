{
  "name": "chci.pages.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:semantic-ui && npm run build:next",
    "build:next": "next build",
    "build:semantic-ui": "cd semantic && npx gulp build-css && npx gulp build-assets && npm run copy:semantic-ui",
    "copy:semantic-ui": "copyfiles -u 2 \"semantic/dist/semantic.min.css\" \"semantic/dist/themes/**/*\" public/",
    "prettify": "prettier --write wrangler.jsonc components pages",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "fomantic-ui": "^2.9.4",
    "next": "^16.1.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "semantic-ui-react": "^3.0.0-beta.2",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "copyfiles": "^2.4.1",
    "prettier": "^3.8.1",
    "wrangler": "^4.60.0"
  }
}
